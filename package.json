{
  "name": "e-bookjs",
  "version": "1.0.0",
  "description": "",
  "main": "src/frameworks/server.js",
  "scripts": {
    "start": "node src/frameworks/server.js",
    "dev": "nodemon --ignore test/ -L src/frameworks/server.js",
    "test": "cross-env NODE_ENV=test jest --watchAll --detectOpenHandles",
    "ci_test": "cross-env NODE_ENV=test jest --forceExit --detectOpenHandles",
    "migrate:make": "knex --cwd=src/frameworks migrate:make",
    "migrate": "knex --cwd=src/frameworks migrate:latest",
    "migrate:rollback": "knex --cwd=src/frameworks migrate:rollback",
    "migrate:refresh": "npm run migrate:rollback && npm run migrate"
  },
