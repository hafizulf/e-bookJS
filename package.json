{
  "name": "e-bookjs",
  "version": "1.0.0",
  "description": "",
  "main": "src/frameworks/webserver/server.js",
  "scripts": {
    "start": "node src/frameworks/webserver/server.js",
    "dev": "nodemon --ignore test/ -L src/frameworks/webserver/server.js",
    "test": "cross-env NODE_ENV=test mocha 'test/**/*.spec.js' --runInBand --exit",
    "test:watch": "cross-env NODE_ENV=test mocha 'test/**/*.spec.js' --watch --runInBand --exit",
    "test:coverage": "nyc --reporter=text --reporter=html cross-env NODE_ENV=test mocha 'test/**/*.spec.js' --exit",
    "migrate:make": "knex --cwd=src/frameworks/database migrate:make",
    "migrate": "knex --cwd=src/frameworks/database migrate:latest",
    "migrate:rollback": "knex --cwd=src/frameworks/database migrate:rollback",
    "migrate:refresh": "npm run migrate:rollback && npm run migrate",
    "migrate:test": "cross-env NODE_ENV=test knex --cwd=src/frameworks/database migrate:latest",
    "migrate:test-rollback": "cross-env NODE_ENV=test knex --cwd=src/frameworks/database migrate:rollback",
    "seed:make": "knex --cwd=src/frameworks/database seed:make",
    "seed": "knex --cwd=src/frameworks/database seed:run",
    "lint": "eslint ./src --ext .js",
    "lint:fix": "eslint --fix ./src --ext .js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.1",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "node-cache": "^5.1.2",
    "path": "^0.12.7",
    "sqlite3": "^5.1.6",
    "swagger-ui-express": "^4.6.2",
    "uuid": "^9.0.0",
    "yaml": "^2.2.1",
    "yup": "^1.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "chai": "^4.3.7",
    "eslint": "^8.33.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0",
    "sinon": "^15.0.1",
    "supertest": "^6.3.3"
  }
}
